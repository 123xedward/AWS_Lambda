{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "iam:CreateInstanceProfile",
                "iam:DeleteInstanceProfile",
                "iam:CreateGroup",
                "iam:DeleteGroup",
                "iam:CreateRole",
                "iam:DeleteRole",
                "iam:DeleteUser",
                "iam:CreateUser",
                "iam:AddRoleToInstanceProfile",
                "iam:CreateAccessKey",
                "iam:CreateLoginProfile",
                "iam:AddUserToGroup",
                "iam:DeleteLoginProfile",
                "iam:ChangePassword"
            ],
            "Resource": [
                "arn:aws:iam::*:instance-profile/*",
                "arn:aws:iam::*:user/*",
                "arn:aws:iam::*:role/*",
                "arn:aws:iam::*:group/*"
            ]
        }
    ]
}